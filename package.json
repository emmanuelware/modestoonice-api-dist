{
  "name": "modestoonice-api",
  "description": "Modesto On Ice API",
  "author": "LaMar Software",
  "license": "UNLICENSED",
  "version": "1.7.0",
  "repository": {
    "type": "git",
    "url": "",
    "dupe": "jscpd -f '**/*.ts' -e '**/node_modules/**'"
  },
  "private": true,
  "main": "app.js",
  "scripts": {
    "pretty": "prettier --write --single-quote '**/*.ts' --print-width 120",
    "dupe": "jscpd -f '**/*.ts' -e '**/node_modules/**'",
    "tsc": "tsc",
    "compile": "npm run tsc && node -r dotenv/config ./dist/app.js dotenv_config_path=./.env",
    "dev": "nodemon -e ts --exec \"npm run compile\"",
    "spellcheck": "cspell 'src/**/*.ts'",
    "dev-ar": "./node_modules/nodemon/bin/nodemon.js -e ts --exec \"npm run compile-ar\"",
    "compile-ar": "npm run tsc-ar && node test-v2.js",
    "tsc-ar": "tsc --pretty --lib 'dom','es2015','es2017' --types 'node' test-v2.ts",
    "build": "tsc -p tsconfig.json && cd .. && cd modestoonice-api-dist && git pull && rm -rf * && cp -R ../modestoonice-api/dist/* ../modestoonice-api/package.json ../modestoonice-api/package-lock.json . && git add -A && git commit -m 'Chore; prod' && git push && echo 'Done - prod'"
  },
  "engines": {
    "node": "~8.2.1"
  },
  "dependencies": {
    "@sendgrid/mail": "~6.4.0",
    "@sentry/node": "~4.6.4",
    "@types/koa-morgan": "^1.0.5",
    "aws-sdk": "~2.358.0",
    "axios": "^0.19.0",
    "body-parser": "~1.18.3",
    "bunyan": "~1.8.12",
    "child-process-promise": "~2.2.1",
    "chrono-node": "~1.3.11",
    "crypto": "^1.0.1",
    "debug": "~2.6.1",
    "dotenv": "~6.2.0",
    "exceljs": "^4.2.1",
    "forever": "~0.15.3",
    "http": "0.0.0",
    "isobject": "~3.0.1",
    "jsonwebtoken": "~8.4.0",
    "koa": "~2.7.0",
    "koa-body": "~4.0.8",
    "koa-bunyan": "~1.0.2",
    "koa-compose": "~4.1.0",
    "koa-compress": "~3.0.0",
    "koa-lusca": "~2.2.0",
    "koa-morgan": "^1.0.1",
    "koa-multer": "~1.0.2",
    "koa-route": "~3.2.0",
    "koa-router": "~7.4.0",
    "koa-send": "~5.0.0",
    "koa-static": "~5.0.0",
    "koa2-cors": "^2.0.6",
    "koa2-formidable": "~1.0.2",
    "mailchimp": "^1.2.1",
    "moment": "~2.24.0",
    "mysql": "~2.16.0",
    "mysql2": "~1.6.5",
    "mz": "~2.6.0",
    "nodejs-info": "~1.2.0",
    "pdfkit": "^0.9.1",
    "randomstring": "~1.1.5",
    "rxjs": "^6.5.3",
    "scrypt": "~6.0.3",
    "square": "^9.1.0",
    "utils": "~0.3.1"
  },
  "devDependencies": {
    "@types/node": "~10.14.4",
    "@types/pdfkit": "^0.10.3",
    "apidoc": "~0.17.5",
    "babel-eslint": "~7.2.3",
    "chai": "~3.5.0",
    "eslint": "~3.19.0",
    "mocha": "~3.2.0",
    "nodemon": "~1.18.10",
    "npm-check": "~5.4.0",
    "npm-check-updates": "~2.10.3",
    "prettier": "1.15.2",
    "supertest": "~3.0.0",
    "typescript": "^3.7.7"
  }
}
